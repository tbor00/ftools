{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "lib": {
            "dependsOn": ["^lib"],
            "outputs": ["lib/**"]
        },
        "format": {
            "outputs": [],
            "inputs": ["packages/**/*"]
        },
        "test": {
            "outputs": [],
            "inputs": ["tests/**/*.js", "tests/**/*.jsx"]
        },
        "deploy": {
            "dependsOn": ["lib", "test", "lint"],
            "outputs": []
        }
    }
}
