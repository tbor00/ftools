{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "lib": {
            "dependsOn": ["^lib"],
            "outputs": ["lib/**"]
        },
        "test": {
            "dependsOn": ["lib"],
            "outputs": [],
            "inputs": ["src/**/*.js", "src/**/*.ts", "test/**/*.js", "test/**/*.jsx"]
        },
        "deploy": {
            "dependsOn": ["lib", "test", "lint"],
            "outputs": []
        }
    }
}
