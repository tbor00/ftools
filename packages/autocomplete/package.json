{
    "name": "@ftools-suit/autocomplete",
    "version": "1.0.3",
    "description": "Autocomplete and geocoding functions/hooks of google maps",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/tbor00/ftools"
    },
    "author": "@tbor00",
    "license": "MIT",
    "homepage": "https://github.com/tbor00/ftools/tree/main/packages/autocomplete#readme",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/*",
        "README.md"
    ],
    "exports": {
        ".": "./lib/index.js",
        "./react": "./lib/react.js",
        "./geocode": "./lib/geocode.js"
    },
    "typesVersions": {
        "*": {
            "react": [
                "./lib/react.d.ts"
            ],
            "geocode": [
                "./lib/geocode.d.ts"
            ]
        }
    },
    "scripts": {
        "lib": "tsc && babel ./src --out-dir ./lib --extensions '.ts'",
        "format": "prettier --write ./src"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.18.6",
        "@types/googlemaps": "^3.43.3",
        "react": "18.2.0"
    },
    "peerDependencies": {
        "react": ">=17"
    },
    "babel": {
        "extends": "../../babel.config.json",
        "presets": [
            "@babel/preset-react",
            "@babel/preset-typescript"
        ]
    }
}
